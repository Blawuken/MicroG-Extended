<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <Preference
        android:layout="@layout/uwu_lonte_playstore"
        android:selectable="false" />

    <org.microg.gms.ui.SwitchBarPreference
        android:icon="@drawable/uwu_on_of"
        android:layout="@layout/uwu_top_switch"
        android:widgetLayout="@layout/preference_widget_switch"
        android:title="@string/pref_play_licensing_enable_switch"
        android:key="play_licensing"
        android:persistent="false" />

    <PreferenceCategory android:layout="@layout/preference_category_no_label">
